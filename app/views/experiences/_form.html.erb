<%= form_with(model: experience, local: true) do |form| %>
  <% if experience.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(experience.errors.count, "error") %> prohibited this experience from being saved:</h2>

      <ul>
      <% experience.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
 <div class="field">
    
    <%= form.file_field :experience, multipart: true, class: 'form-control' %>
  </div>
  <div class="field">
    <%= form.label :owner %>
    <%= form.number_field :owner, id: :experience_owner, class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :start_date %>
    <%= form.text_field :start_date, id: :experience_start_date, class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :end_date %>
    <%= form.text_field :end_date, id: :experience_end_date, class: 'form-control'%>
  </div>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :experience_name, class: 'form-control' %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_field :description, id: :experience_description, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit  class: 'btn btn-primary' %>
  </div>
<% end %>
